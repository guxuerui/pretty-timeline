{
  "name": "pretty-timeline",
  "version": "1.0.0",
  "author": "guxuerui",
  "description": "vue3 pretty timeline",
  "packageManager": "pnpm@7.1.5",
  "keywords": [
    "vue3",
    "Vue3",
    "pretty",
    "timeline"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/guxuerui/pretty-timeline"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/pretty-timeline.umd.ts",
  "module": "./dist/pretty-timeline.es.ts",
  "exports": {
    ".": {
      "import": "./dist/pretty-timeline.es.ts",
      "require": "./dist/pretty-timeline.umd.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3334",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@vueuse/core": "v9.1.0",
    "@vueuse/head": "v0.7.9",
    "axios": "^1.2.0",
    "canvas-confetti": "^1.6.0",
    "markdown-it-prism": "^2.3.0",
    "vite-plugin-vue-markdown": "0.22.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@iconify-json/carbon": "^1.1.11",
    "@iconify-json/mdi": "^1.1.36",
    "@types/canvas-confetti": "^1.6.0",
    "@types/node": "^18.11.10",
    "@unocss/reset": "^0.47.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/test-utils": "^2.2.5",
    "eslint": "^8.28.0",
    "jsdom": "^20.0.3",
    "pnpm": "^7.17.1",
    "prismjs": "^1.29.0",
    "typescript": "^4.9.3",
    "unocss": "^0.47.5",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.4",
    "vite-plugin-pages": "^0.27.1",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.10"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
